{
  "name": "date-planner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run data:generate && vite",
    "build": "npm run data:generate && npm run typecheck && vite build",
    "data:generate": "node scripts/generatePublicData.mjs",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/google.maps": "^3.58.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "gh-pages": "^6.3.0",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
